{
	"settings": {
		"number_of_shards": 1
	},
	"mappings": {
		"event": {
			"properties": {
				"value": {
					"type": "keyword"
				},
				"device_id": {
					"type": "keyword"
				},
				"latency": {
					"type": "float"
				},
				"device_name": {
					"type": "keyword"
				},
				"source": {
					"type": "keyword"
				},
				"missedFlag": {
					"type": "keyword"
				},
				
				"unix_time": {
					"type": "date",
					"format": "strict_date_optional_time||epoch_millis"
				}
			}
		}
	}
}
